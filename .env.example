# GoTrip Backend Environment Variables

# Server Configuration
PORT=3000
NODE_ENV=development

# MySQL Database Configuration (Railway Cloud Database)
# Ask team lead for actual credentials
DB_HOST=your-railway-host.railway.app
DB_USER=root
DB_PASSWORD=ask-team-lead-for-password
DB_NAME=railway
DB_PORT=your-railway-port

# JWT Secret (for authentication tokens)
JWT_SECRET=your_super_secret_jwt_key_change_this_in_production

# Session Secret
SESSION_SECRET=your_session_secret_key_change_this

# Frontend URL (for CORS)
FRONTEND_URL=http://localhost:3000
